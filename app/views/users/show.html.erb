<h1>Mostrar Usuario</h1>
<table class="table table-striped" summary="Show Task" >
    <tr class="header">
        <th></th>
        <th>Contenido</th>
    </tr>
    <tr>
            <th>Nombre</th>
            <%= form_for(:user, :url => {:action => 'show', :id => user.id}) do |f| %>
            <td><%= f.first_name %></td>
    </tr>
    <tr>
            <th>Apellido</th>
            <td><%= f.last_name %></td>
    </tr>
    <tr>
            <th>Telefono</th>
            <td><%= f.phone %></td>
    </tr>
    <tr>
            <th>Email</th>
            <td><%= f.email %></td>
    </tr>
 
</table>
        <% end %>